{
    "name": "eplengine",
    "version": "1.0.1",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "esm/index.js",
    "scripts": {
        "watch": "tsc --watch",
        "watch:cjs": "tsc --watch -p tsconfig.cjs.json",
        "build": "npm run build:all",
        "build:all": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "prepare": "npm run build:all",
        "prepublish": "npm run build:all"
    },
    "author": "Kate Ward <kate@dariox.club>",
    "license": "AGPL-3.0",
    "dependencies": {
        "events": "^3.3.0",
        "pixi.js": "^6.3.0"
    },
    "devDependencies": {
        "@types/events": "^3.0.0",
        "@types/pixi.js": "^5.0.0",
        "typescript": "^4.6.4"
    }
}
